#include <bits/stdc++.h>

using namespace std;
int main(){
    int s,t;
    cin>>s>>t;
    int a,b;
    cin>>a>>b;
    int m,n;
    cin>>m>>n;
    int a1[m],b1[n];
    int apple=0,orange=0;
    
    for(int i=0;i<m;i++)
    {  cin>>a1[i];
        
            if(a+a1[i]>=s&&a+a1[i]<=t)
            apple++;  
    }
    
    for(int i=0;i<n;i++)
    {  cin>>b1[i];
        
            if(b+b1[i]>=s&&b+b1[i]<=t)
            orange++;    
    }
    cout<<apple<<endl;
    cout<<orange<<endl;
    
    return 0;
}
